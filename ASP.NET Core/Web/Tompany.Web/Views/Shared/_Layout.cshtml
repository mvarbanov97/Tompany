@using Tompany.Common
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@this.ViewData["Title"] - @GlobalConstants.SystemName</title>

    <link rel="icon" href="~/favicon.ico" type="image/x-icon" />
    <link rel="apple-touch-icon-precomposed" sizes="144x144" href="~/favicon.ico">
    <link rel="apple-touch-icon-precomposed" sizes="72x72" href="~/favicon.ico">
    <link rel="apple-touch-icon-precomposed" href="~/favicon.ico">
    <link rel="stylesheet" href="~/css/tripCardStyle.css">
    <link rel="stylesheet" href="~/css/bootstrap.min.css">
    <link rel="stylesheet" href="~/css/preset.css">
    <link rel="stylesheet" href="~/css/chat.css">
    <link rel="stylesheet" href="~/css/style.css">
    <link rel="stylesheet" href="~/css/responsive.css">
    <link rel="stylesheet" href="~/css/font-awesome.min.css">
    <link rel="stylesheet" href="~/css/animate.css">
    <link rel="stylesheet" href="~/css/prettyPhoto.css">
    <link rel="stylesheet" href="~/css/owl.carousel.css">
    <link rel="stylesheet" href="~/css/owl.theme.css">
    <link rel="stylesheet" href="~/css/flexslider.css">
    <link rel="stylesheet" href="~/css/cd-hero.css">
    <link rel="stylesheet" href="~/css/profileStyle.css">
    <link rel="stylesheet" href="~/css/userCard.css">
    <link rel="stylesheet" href="~/css/chooseFile.css">
    <link rel="stylesheet" href="~/css/multiSelect.css">
    <link rel="stylesheet" href="~/css/searchField.css">
    <link rel="stylesheet" href="~/css/notificaitonButton.css">
    <link rel="stylesheet" href="~/css/profileRating.css">
    <link rel="stylesheet" href="~/css/tripDetails.css">
    <link rel="stylesheet" href="~/notification/css/notificationStyle.css">
    <link rel="stylesheet" href="~/css/userStatusStyle.css">

</head>
<body>
    <div class="body-inner">
        <header id="header" class="navbar-fixed-top header2" role="banner">
            <div class="container">
                <div class="row">
                    <div class="navbar-header">
                        <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                            <span class="sr-only">Toggle navigation</span>
                            <span class="icon-bar"></span>
                            <span class="icon-bar"></span>
                            <span class="icon-bar"></span>
                        </button>
                        <div class="navbar-brand">
                            <a asp-area="" asp-controller="Home" asp-action="Index">
                                <img class="img-responsive" style="width: 270px;" src="~/images/LogoMakr_1gjyGP.png" alt="logo">
                            </a>
                        </div>
                    </div>

                    <nav class="collapse navbar-collapse" role="navigation">
                        <ul class="nav navbar-nav navbar-right">
                            <li>
                                <a asp-area="" asp-controller="Home" asp-action="Index">Home</a>
                            </li>
                            <li>
                                <div class="megamenu">
                                    <div class="dropdown-menu">
                                        <a class="megamenu" asp-area="" asp-controller="Trips" asp-action="Index">Trips</a>
                                        <a class="megamenu" asp-area="" asp-controller="Trips" asp-action="Index">Trips</a>
                                        <a class="megamenu" asp-area="" asp-controller="Trips" asp-action="Index">Trips</a>
                                        <a class="megamenu" asp-area="" asp-controller="Trips" asp-action="Index">Trips</a>

                                    </div>
                                </div>
                            </li>

                            <li>
                                <a class="megamenu" asp-area="" asp-controller="Trips" asp-action="Index">Trips</a>
                            </li>
                            <li>
                                <a asp-area="" asp-controller="Contacts" asp-action="Index">Contact</a>
                            </li>
                            <partial name="_LoginPartial" />
                        </ul>
                    </nav>
                </div>
            </div>
        </header>
        @*<section class="renderBodySection">*@
        @RenderBody()
        @*</section>*@

        <footer id="footer" class="footer footer2">
            <div class="container">

                <div class="row">
                    <div class="col-md-6 col-sm-12 footer-widget">
                        <h3 class="widget-title">Quick Links</h3>

                        <ul class="unstyled arrow">
                            <li><a asp-area="" asp-controller="Home" asp-action="Index">Home</a></li>
                            <li><a asp-area="" asp-controller="Contact" asp-action="Index">Contact</a></li>
                            <partial name="_FooterLoginPartial" />
                        </ul>
                    </div>
                    <div class="col-md-6 col-sm-12 footer-widget footer-about-us">
                        <h3 class="widget-title">About Us</h3>
                        <p>We are a Bulgarian startup company. We believe that travelling should be fun and cheap.</p>
                        <p><strong>Address: </strong>Sofia, Bulgaria</p>
                        <div class="row">
                            <div class="col-md-6">
                                <strong>Email: </strong>
                                <p>momchil.varbanov97@gmail.com</p>
                            </div>
                            <div class="col-md-7">
                                <strong>Phone:</strong>
                                <p>(+359) 89 548 0865</p>
                            </div>
                        </div>
                        <br />
                    </div>
                </div>
            </div>
        </footer>
        <section id="copyright" class="copyright angle">
            <div class="container">
                <div class="row">
                    <div class="col-md-12 text-center">
                        <ul class="footer-social unstyled">
                            <li>
                                <a title="Facebook" href="#">
                                    <span class="icon-pentagon wow bounceIn"><i class="fab fa-facebook"></i></span>
                                </a>
                                <a title="linkedin" href="#">
                                    <span class="icon-pentagon wow bounceIn"><i class="fab fa-linkedin"></i></span>
                                </a>
                                <a title="Skype" href="#">
                                    <span class="icon-pentagon wow bounceIn"><i class="fab fa-skype"></i></span>
                                </a>
                            </li>
                        </ul>
                    </div>
                </div>
                <div class="row">
                    <div class="col-md-12 text-center">
                        <div class="copyright-info">
                            &copy; Copyright 2020 Tompany
                        </div>
                    </div>
                </div>
                <div id="back-to-top" data-spy="affix" data-offset-top="10" class="back-to-top affix">
                    <button class="btn btn-primary" title="Back to Top"><i class="fas fa-angle-double-up"></i></button>
                </div>
            </div>
        </section>

        <script src="https://cdnjs.cloudflare.com/ajax/libs/three.js/r79/three.min.js"></script>

        <script src="~/lib/jquery/dist/jquery.min.js"></script>
        <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>

        <script src="~/js/site.js" asp-append-version="true"></script>
        <script type="text/javascript" src="~/js/jquery.js"></script>

        <script type="text/javascript" src="~/js/bootstrap.min.js"></script>
        <script type="text/javascript" src="~/js/owl.carousel.js"></script>
        <script type="text/javascript" src="~/js/jquery.prettyPhoto.js"></script>
        <script type="text/javascript" src="~/js/jquery.flexslider.js"></script>
        <script type="text/javascript" src="~/js/cd-hero.js"></script>
        <script type="text/javascript" src="~/js/isotope.js"></script>
        <script type="text/javascript" src="~/js/ini.isotope.js"></script>
        <script type="text/javascript" src="~/js/wow.min.js"></script>
        <script type="text/javascript" src="~/js/smoothscroll.js"></script>
        <script type="text/javascript" src="~/js/jquery.easing.1.3.js"></script>
        <script type="text/javascript" src="~/js/jquery.counterup.min.js"></script>
        <script type="text/javascript" src="~/js/waypoints.min.js"></script>
        <script type="text/javascript" src="~/js/custom.js"></script>
        <script type="text/javascript" src="~/js/profileScripts.js"></script>
        <script type="text/javascript" src="~/js/layoutScript.js"></script>
        <script type="text/javascript" src="~/js/notify.js"></script>
        <script src="https://kit.fontawesome.com/b0b195894f.js" crossorigin="anonymous"></script>
        <script src="~/js/signalr/dist/browser/signalr.js" asp-append-version="true"></script>
        <script src="~/lib/moment/min/moment-with-locales.min.js" asp-append-version="true"></script>
        <script type="text/javascript" src="~/notification/js/notification.js"></script>
        <script type="text/javascript" src="~/js/userStatus.js"></script>

        @this.RenderSection("Scripts", required: false)
    </div>
</body>
</html>
